{
  "name": "electron-ota-backend",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/codemotion/electron-ota.git"
  },
  "scripts": {
    "dev": "ts-node-dev src/app.ts",
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/app.js",
    "watch-node": "nodemon dist/app.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect=0.0.0.0:9229 dist/app.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "aws-sdk": "2.377.0",
    "bcrypt": "3.0.2",
    "bind-decorator": "1.0.11",
    "bluebird": "3.5.3",
    "dotenv": "6.1.0",
    "inversify": "5.0.1",
    "joi": "14.3.1",
    "jsonwebtoken": "8.4.0",
    "mongoose": "5.4.9",
    "reflect-metadata": "0.1.12",
    "semver": "5.6.0",
    "socket.io": "2.2.0",
    "tmp": "0.0.33",
    "tsdv-joi": "0.0.11",
    "typegoose": "5.5.0",
    "winston": "2.4.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.24",
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.0",
    "@types/jest": "^23.3.10",
    "@types/joi": "10.4.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mongodb": "^3.1.16",
    "@types/mongoose": "^5.3.2",
    "@types/node": "^10.12.12",
    "@types/semver": "^5.5.0",
    "@types/socket.io": "^2.1.0",
    "@types/supertest": "^2.0.6",
    "@types/tmp": "^0.0.33",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2"
  },
  "resolutions": {
    "typegoose/mongoose": "5.4.9"
  }
}
