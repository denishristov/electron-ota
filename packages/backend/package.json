{
  "name": "backend",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/codemotion/electron-ota.git"
  },
  "scripts": {
    "dev": "ts-node-dev ./src/",
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node --trace-warnings ./dist/",
    "watch-node": "nodemon ./dist/",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "npx ttsc",
    "watch-ts": "npx ttsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect=0.0.0.0:9229 ./dist",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@sentry/node": "4.5.3",
    "aws-sdk": "2.377.0",
    "bcryptjs": "2.4.3",
    "bind-decorator": "1.0.11",
    "bluebird": "3.5.3",
    "body-parser": "^1.18.3",
    "chalk": "2.4.2",
    "cookie-parser": "^1.4.4",
    "cors": "2.8.5",
    "dotenv": "6.1.0",
    "express": "^4.16.4",
    "invariant": "2.2.4",
    "inversify": "5.0.1",
    "inversify-express-utils": "^6.3.2",
    "joi": "14.3.1",
    "jsonwebtoken": "8.4.0",
    "mongoose": "5.4.9",
    "reflect-metadata": "0.1.12",
    "semver": "5.6.0",
    "socket.io": "2.2.0",
    "socket.io-cookie-parser": "1.0.0",
    "socket.io-redis": "5.2.0",
    "tsdv-joi": "0.0.11",
    "typegoose": "5.5.0",
    "winston": "2.4.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.24",
    "@types/body-parser": "^1.17.0",
    "@types/chalk": "^2.2.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^23.3.10",
    "@types/joi": "10.4.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mongodb": "^3.1.16",
    "@types/mongoose": "^5.3.2",
    "@types/node": "^10.12.12",
    "@types/semver": "^5.5.0",
    "@types/socket.io": "^2.1.0",
    "@types/socket.io-redis": "^1.0.25",
    "@types/supertest": "^2.0.6",
    "@types/tmp": "^0.0.33",
    "@types/ts-nameof": "^2.0.0",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-nameof": "^3.0.3",
    "ts-node-dev": "^1.0.0-pre.32",
